.DEFAULT_GOAL := all
.PHONY: all
CFLAGS= -W -Wall -Wextra

all: main2 main3 makegen 

MAIN2OBJ= ./dot_helper.o ./tree.o ./string_helper.o ./file_helper.o ./linked_list.o ./make_helper.o ./test1.o ./test/test1-Copy.o ./main2.o 

MAIN2DEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./linked_list.h ./make_helper.h ./test1.h ./test/wasd/wasd.h ./test/test1-Copy.h 

main2.c: $(MAIN2OBJ) ($MAIN2DEP)

MAIN3OBJ= ./dot_helper.o ./tree.o ./string_helper.o ./file_helper.o ./make_helper.o ./linked_list.o ./main3.o 

MAIN3DEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./make_helper.h ./linked_list.h 

main3.c: $(MAIN3OBJ) ($MAIN3DEP)

MAKEGENOBJ= ./dot_helper.o ./tree.o ./string_helper.o ./file_helper.o ./make_helper.o ./linked_list.o ./makegen.o 

MAKEGENDEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./make_helper.h ./linked_list.h 

makegen.c: $(MAKEGENOBJ) ($MAKEGENDEP)

clean:
	$(RM) main2 main3 makegen *.o