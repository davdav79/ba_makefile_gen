.DEFAULT_GOAL := all
.PHONY: all
CFLAGS= -W -Wall -Wextra

all: main2 main3 makegen 

####################MAIN2####################

MAIN2OBJ= ./dot_helper.c ./tree.c ./string_helper.c ./file_helper.c ./linked_list.c ./make_helper.c ./test1.c ./test/test1-Copy.c 

MAIN2DEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./linked_list.h ./make_helper.h ./test1.h ./test/wasd/wasd.h ./test/test1-Copy.h 

main2: $(MAIN2OBJ)

####################MAIN3####################

MAIN3OBJ= ./dot_helper.c ./tree.c ./string_helper.c ./file_helper.c ./make_helper.c ./linked_list.c 

MAIN3DEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./make_helper.h ./linked_list.h 

main3: $(MAIN3OBJ)

####################MAKEGEN####################

MAKEGENOBJ= ./dot_helper.c ./tree.c ./string_helper.c ./file_helper.c ./make_helper.c ./linked_list.c 

MAKEGENDEP= ./dot_helper.h ./tree.h ./string_helper.h ./file_helper.h ./make_helper.h ./linked_list.h 

makegen: $(MAKEGENOBJ)

clean:
	$(RM) main2 main3 makegen *.o