.DEFAULT_GOAL := all
.PHONY: all
CFLAGS= -W -Wall -Wextra

all: makegen 

####################MAKEGEN####################

MAKEGENOBJ= ./tree.c ./string_helper.c ./file_helper.c ./linked_list.c ./make_helper.c ./argp_helper.c ./dot_helper.c 

MAKEGENLIB= -lpthread -lm 

makegen: $(MAKEGENOBJ) ($MAKEGENLIB)

clean:
	$(RM) makegen *.o